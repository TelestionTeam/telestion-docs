{
  "name": "dita-ot-helper-boilerplate",
  "private": true,
  "version": "1.3.0",
  "scripts": {
    "start": "npm run build",
    "build": "dita-ot-helper --verbose -i 3.6.0 build-configs/\\*.json",
    "prepare": "fliegdoc build",
    "release": "standard-version"
  },
  "author": "Pablo Klaschka",
  "license": "MIT",
  "dependencies": {
    "@wuespace/telestion-client-common": "^0.10.0",
    "@wuespace/telestion-client-core": "^0.11.1",
    "dita-ot-helper": "^0.13.0",
    "fliegdoc": "^0.4.7",
    "@fliegwerk/fliegdoc-dita-theme": "^0.0.6"
  },
  "devDependencies": {
    "standard-version": "^9.1.1"
  },
  "standard-version": {
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "chore",
        "section": "Other"
      },
      {
        "type": "docs",
        "section": "Features"
      },
      {
        "type": "style",
        "hidden": true
      },
      {
        "type": "refactor",
        "hidden": true
      },
      {
        "type": "perf",
        "hidden": true
      },
      {
        "type": "test",
        "hidden": true
      }
    ]
  }
}
